{
	"name": "backend_nodejs",
	"version": "1.0.0",
	"description": "",
	"main": "index.js",
	"scripts": {
		"build": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files --no-copy-ignored",
		"start": "node dist/",
		"dev": "ts-node-dev -r tsconfig-paths/register --respawn --transpile-only --ignore-watch node_modules --no-notify src/index.ts",
		"test": "echo under development",
		"knex": "knex --knexfile database/knexfile.ts --env main",
		"knex:migration:add": "knex --knexfile database/knexfile.ts --env main migrate:make",
		"knex:seed:add": "knex --knexfile database/knexfile.ts --env main seed:make"
	},
	"author": "Almir Junior",
	"license": "MIT",
	"devDependencies": {
		"@types/cors": "^2.8.12",
		"@types/express": "^4.17.13",
		"@types/mocha": "^9.0.0",
		"@types/mochawesome": "^6.2.1",
		"@types/node": "^16.11.11",
		"@typescript-eslint/eslint-plugin": "^5.5.0",
		"@typescript-eslint/parser": "^5.5.0",
		"babel-cli": "^6.26.0",
		"eslint": "^8.4.0",
		"eslint-config-airbnb-base": "^15.0.0",
		"eslint-plugin-import": "^2.25.3",
		"mocha": "^9.1.3",
		"mochawesome": "^7.0.1",
		"ts-node": "^10.4.0",
		"ts-node-dev": "^1.1.8",
		"tsconfig-paths": "^3.12.0",
		"tslib": "^2.3.1",
		"typescript": "^4.5.2"
	},
	"dependencies": {
		"cors": "^2.8.5",
		"dotenv": "^10.0.0",
		"express": "^4.17.1",
		"helmet": "^4.6.0",
		"knex": "^0.95.14",
		"pg": "^8.7.1"
	}
}
